# ETL Pipeline Environment Variables

# PostgreSQL Configuration
POSTGRES_DB_DEFAULT=postgres
POSTGRES_USER=okr_admin
POSTGRES_PASSWORD=okr_password
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=kafka:9092

# Airflow Configuration
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__DAGS_ARE_PAUSED_AT_CREATION=False
AIRFLOW__CORE__DEFAULT_TIMEZONE=UTC

# Optional: Override default configuration paths
# PIPELINE_CONFIG_PATH=/opt/airflow/configs/pipeline_config.json
# DATA_RAW_PATH=/opt/airflow/data/raw